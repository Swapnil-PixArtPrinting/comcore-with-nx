{
  "compilerOptions": {
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "resolvePackageJsonExports": true,
    "esModuleInterop": true,
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2023",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@app/awskit": [
        "libs/awskit/src"
      ],
      "@app/awskit/*": [
        "libs/awskit/src/*"
      ],
      "@app/clientskit": [
        "libs/clientskit/src"
      ],
      "@app/clientskit/*": [
        "libs/clientskit/src/*"
      ],
      "@app/common": [
        "libs/common/src"
      ],
      "@app/common/*": [
        "libs/common/src/*"
      ],
      "@app/corecommerce": [
        "libs/corecommerce/src"
      ],
      "@app/corecommerce/*": [
        "libs/corecommerce/src/*"
      ]
    }
  }
}