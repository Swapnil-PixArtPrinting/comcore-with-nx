{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/comcore3/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/comcore3/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/comcore3",
  "projects": {
    "aws": {
      "type": "library",
      "root": "libs/aws",
      "entryFile": "index",
      "sourceRoot": "libs/aws/src",
      "compilerOptions": {
        "tsConfigPath": "libs/aws/tsconfig.lib.json"
      }
    },
    "clients": {
      "type": "library",
      "root": "libs/clients",
      "entryFile": "index",
      "sourceRoot": "libs/clients/src",
      "compilerOptions": {
        "tsConfigPath": "libs/clients/tsconfig.lib.json"
      }
    },
    "comcore3": {
      "type": "application",
      "root": "apps/comcore3",
      "entryFile": "main",
      "sourceRoot": "apps/comcore3/src",
      "compilerOptions": {
        "tsConfigPath": "apps/comcore3/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "corecommerce": {
      "type": "library",
      "root": "libs/corecommerce",
      "entryFile": "index",
      "sourceRoot": "libs/corecommerce/src",
      "compilerOptions": {
        "tsConfigPath": "libs/corecommerce/tsconfig.lib.json"
      }
    },
    "customer-service": {
      "type": "application",
      "root": "apps/customer-service",
      "entryFile": "main",
      "sourceRoot": "apps/customer-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/customer-service/tsconfig.app.json"
      }
    },
    "order-service": {
      "type": "application",
      "root": "apps/order-service",
      "entryFile": "main",
      "sourceRoot": "apps/order-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/order-service/tsconfig.app.json"
      }
    },
    "sample": {
      "type": "library",
      "root": "libs/sample",
      "entryFile": "index",
      "sourceRoot": "libs/sample/src",
      "compilerOptions": {
        "tsConfigPath": "libs/sample/tsconfig.lib.json"
      }
    }
  }
}