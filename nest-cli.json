{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/comcore4/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/comcore4/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/comcore4",
  "projects": {
    "awskit": {
      "type": "library",
      "root": "libs/awskit",
      "entryFile": "index",
      "sourceRoot": "libs/awskit/src",
      "compilerOptions": {
        "tsConfigPath": "libs/awskit/tsconfig.lib.json"
      }
    },
    "clientskit": {
      "type": "library",
      "root": "libs/clientskit",
      "entryFile": "index",
      "sourceRoot": "libs/clientskit/src",
      "compilerOptions": {
        "tsConfigPath": "libs/clientskit/tsconfig.lib.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "corecommerce": {
      "type": "library",
      "root": "libs/corecommerce",
      "entryFile": "index",
      "sourceRoot": "libs/corecommerce/src",
      "compilerOptions": {
        "tsConfigPath": "libs/corecommerce/tsconfig.lib.json"
      }
    },
    "customer": {
      "type": "application",
      "root": "apps/customer",
      "entryFile": "main",
      "sourceRoot": "apps/customer/src",
      "compilerOptions": {
        "tsConfigPath": "apps/customer/tsconfig.app.json"
      }
    },
    "order": {
      "type": "application",
      "root": "apps/order",
      "entryFile": "main",
      "sourceRoot": "apps/order/src",
      "compilerOptions": {
        "tsConfigPath": "apps/order/tsconfig.app.json"
      }
    },
    "jobs": {
      "type": "application",
      "root": "apps/jobs",
      "entryFile": "main",
      "sourceRoot": "apps/jobs/src",
      "compilerOptions": {
        "tsConfigPath": "apps/jobs/tsconfig.app.json"
      }
    }
  }
}